<h1 style="color: red">NEW</h1>

<%= form_for :user, url: users_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :age %>
    <%= f.text_field :age %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.submit %>
<% end %>
